      <div class="choice-label"><%= age_label_20 %></div>
      <div class="choice-line-container"><div class="choice-line" style="width: <%= data.age_percent_20 || 0  %>%"></div></div>
    </div>
    <div class="choice-column choice-column-1">
      <div class="choice-label"><%= age_label_25 %></div>
      <div class="choice-line-container"><div class="choice-line" style="width: <%= data.age_percent_25 || 0  %>%"></div></div>
    </div>
    <div class="choice-column choice-column-1">
      <div class="choice-label"><%= age_label_30 %></div>
