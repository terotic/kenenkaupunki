    <div class="choice-column choice-column-1">
      <div class="choice-label"><%= age_label_60 %></div>
      <div class="choice-line-container"><div class="choice-line" style="width: <%= data.age_percent_60  || 0 %>%"></div></div>
    </div>
    <div class="choice-column choice-column-1">
