<div class="summary-header">
  <div class="summary-icon">
      <i class="maki-golf"></i>
  </div>
  <h3><%= age_title %></h3>
</div>
<div class="summary-content">
  <div class="choice-chart choice-chart-noicons">
    <div class="choice-column choice-column-1">
      <div class="choice-label"><%= age_label_0 %></div>
      <div class="choice-line-container"><div class="choice-line" style="width: <%= data.age_percent_0 || 0 %>%"></div></div>
    </div>
    <div class="choice-column choice-column-1">
      <div class="choice-label"><%= age_label_16 %></div>
      <div class="choice-line-container"><div class="choice-line" style="width: <%= data.age_percent_16 || 0 %>%"></div></div>
    </div>
